<ns0:recipe xmlns:ns0="http://fruschtique.de/ns/recipe" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://fruschtique.de/ns/recipe file:///c:/Users/nlutt/Documents/Websites/tools/recipe.xsd" rcpID="ck-a11b55b3-e4c7-4b43-8f60-8bee52724479">
  <ns0:meta>
    <ns0:book />
    <ns0:chapter />
  </ns0:meta>
  <ns0:recipeName>Gef&#252;llte Gr&#252;nkohlkartoffeln</ns0:recipeName>
  <ns0:recipeKeywords />
  <ns0:recipeIntro />
  <ns0:recipeIngredients>
    <ns0:igdtList>
      <ns0:igdtListName />
      <ns0:igdtListLine>
        <ns0:igdtQuantity>1 Beutel</ns0:igdtQuantity>
        <ns0:igdtName ref="gr&#252;nkohl">Gr&#252;nkohl</ns0:igdtName>
      </ns0:igdtListLine>
      <ns0:igdtListLine>
        <ns0:igdtQuantity>8 gro&#223;e</ns0:igdtQuantity>
        <ns0:igdtName ref="kartoffel">Kartoffel(n), festkochend</ns0:igdtName>
      </ns0:igdtListLine>
      <ns0:igdtListLine>
        <ns0:igdtQuantity>150 g</ns0:igdtQuantity>
        <ns0:igdtName ref="speck">Speck, durchwachsen</ns0:igdtName>
      </ns0:igdtListLine>
      <ns0:igdtListLine>
        <ns0:igdtQuantity>2</ns0:igdtQuantity>
        <ns0:igdtName ref="zwiebel">Zwiebel(n)</ns0:igdtName>
      </ns0:igdtListLine>
      <ns0:igdtListLine>
        <ns0:igdtQuantity>4 Zehe/n</ns0:igdtQuantity>
        <ns0:igdtName ref="knoblauch">Knoblauch</ns0:igdtName>
      </ns0:igdtListLine>
      <ns0:igdtListLine>
        <ns0:igdtQuantity>250 ml</ns0:igdtQuantity>
        <ns0:igdtName ref="sahne">Sahne</ns0:igdtName>
      </ns0:igdtListLine>
      <ns0:igdtListLine>
        <ns0:igdtQuantity>&#189; Tasse/n</ns0:igdtQuantity>
        <ns0:igdtName ref="parmesan">Parmesan</ns0:igdtName>
      </ns0:igdtListLine>
      <ns0:igdtListLine>
        <ns0:igdtQuantity />
        <ns0:igdtName ref="oliven&#246;l">Oliven&#246;l</ns0:igdtName>
      </ns0:igdtListLine>
      <ns0:igdtListLine>
        <ns0:igdtQuantity />
        <ns0:igdtName ref="pfeffer">Salz und Pfeffer</ns0:igdtName>
      </ns0:igdtListLine>
      <ns0:igdtListLine>
        <ns0:igdtQuantity />
        <ns0:igdtName ref="muskat">Muskat</ns0:igdtName>
      </ns0:igdtListLine>
    </ns0:igdtList>
  </ns0:recipeIngredients>
  <ns0:recipeInstructions>
    <ns0:instruction>
      <ns0:instrStepName />
      <ns0:instrStepText>Kartoffeln sch&#228;len, eine Kappe abschneiden und aush&#246;hlen. Gr&#252;nkohl von den gro&#223;en Str&#228;ngen schneiden und in kochendem Salzwasser ca. 3 Minuten blanchieren, abschrecken und ausdr&#252;cken. Kleingeschnittene Zwiebeln, Knoblauch und Speck in Oliven&#246;l and&#252;nsten. Pro Person eine halbe Tasse ausgedr&#252;ckten Gr&#252;nkohl dazugeben, mit Sahne aufgie&#223;en und mit Salz, Pfeffer und Muskat w&#252;rzen. Mit der Masse die Kartoffeln f&#252;llen, die F&#252;llung sch&#246;n reindr&#252;cken, mit Parmesan betreuen und bei 200 Grad 35 Minuten im Ofen backen.</ns0:instrStepText>
    </ns0:instruction>
  </ns0:recipeInstructions>
  <ns0:recipeSideDish />
