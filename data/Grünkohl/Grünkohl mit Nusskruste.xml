        <ns0:igdtQuantity>100 g</ns0:igdtQuantity>
        <ns0:igdtName ref="walnuss">Walnusskerne</ns0:igdtName>
      </ns0:igdtListLine>
      <ns0:igdtListLine>
        <ns0:igdtQuantity>100 g</ns0:igdtQuantity>
        <ns0:igdtName ref="bergk&#228;se">Bergk&#228;se oder Gouda</ns0:igdtName>
      </ns0:igdtListLine>
      <ns0:igdtListLine>
        <ns0:igdtQuantity>2</ns0:igdtQuantity>
        <ns0:igdtName ref="ei">Ei(er)</ns0:igdtName>
      </ns0:igdtListLine>
      <ns0:igdtListLine>
        <ns0:igdtQuantity>2</ns0:igdtQuantity>
        <ns0:igdtName ref="knoblauch">Knoblauchzehe(n)</ns0:igdtName>
      </ns0:igdtListLine>
      <ns0:igdtListLine>
