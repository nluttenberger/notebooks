  <ns0:recipeName>Gr&#252;nkohl-Pfanne mit Sp&#228;tzle</ns0:recipeName>
  <ns0:recipeKeywords />
  <ns0:recipeIntro />
  <ns0:recipeIngredients>
    <ns0:igdtList>
      <ns0:igdtListName />
      <ns0:igdtListLine>
        <ns0:igdtQuantity>1</ns0:igdtQuantity>
        <ns0:igdtName ref="knoblauch">Knoblauchzehe(n)</ns0:igdtName>
      </ns0:igdtListLine>
