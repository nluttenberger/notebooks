  <ns0:recipeName>Nudeln mit Gr&#252;nkohl und Feta</ns0:recipeName>
  <ns0:recipeKeywords />
  <ns0:recipeIntro />
  <ns0:recipeIngredients>
    <ns0:igdtList>
      <ns0:igdtListName />
      <ns0:igdtListLine>
        <ns0:igdtQuantity>150 g</ns0:igdtQuantity>
        <ns0:igdtName ref="nudeln">Nudeln</ns0:igdtName>
      </ns0:igdtListLine>
      <ns0:igdtListLine>
        <ns0:igdtQuantity>3 kleine</ns0:igdtQuantity>
        <ns0:igdtName ref="karotte">Karotte(n)</ns0:igdtName>
      </ns0:igdtListLine>
      <ns0:igdtListLine>
        <ns0:igdtQuantity>1 kleiner</ns0:igdtQuantity>
        <ns0:igdtName ref="kohlrabi">Kohlrabi</ns0:igdtName>
      </ns0:igdtListLine>
      <ns0:igdtListLine>
        <ns0:igdtQuantity>1 kg</ns0:igdtQuantity>
        <ns0:igdtName ref="gr&#252;nkohl">Gr&#252;nkohl, gefroren</ns0:igdtName>
      </ns0:igdtListLine>
      <ns0:igdtListLine>
        <ns0:igdtQuantity>1 Pck.</ns0:igdtQuantity>
        <ns0:igdtName ref="schafsk&#228;se">Feta-K&#228;se oder Tofu</ns0:igdtName>
      </ns0:igdtListLine>
      <ns0:igdtListLine>
        <ns0:igdtQuantity>150 ml</ns0:igdtQuantity>
        <ns0:igdtName ref="kokosmilch">Kokosmilch</ns0:igdtName>
      </ns0:igdtListLine>
      <ns0:igdtListLine>
        <ns0:igdtQuantity>120 g</ns0:igdtQuantity>
        <ns0:igdtName ref="saure_sahne">Sauerrahm</ns0:igdtName>
      </ns0:igdtListLine>
      <ns0:igdtListLine>
        <ns0:igdtQuantity>&#189; TL</ns0:igdtQuantity>
        <ns0:igdtName ref="gew&#252;rz">Suppengew&#252;rz</ns0:igdtName>
      </ns0:igdtListLine>
      <ns0:igdtListLine>
        <ns0:igdtQuantity>etwas</ns0:igdtQuantity>
        <ns0:igdtName ref="pfeffer">Salz und Pfeffer</ns0:igdtName>
      </ns0:igdtListLine>
      <ns0:igdtListLine>
        <ns0:igdtQuantity>2 Msp.</ns0:igdtQuantity>
        <ns0:igdtName ref="muskat">Muskatnuss</ns0:igdtName>
      </ns0:igdtListLine>
