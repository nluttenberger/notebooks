        <ns0:igdtName ref="kartoffel">Pellkartoffel(n), vom Vortag</ns0:igdtName>
      </ns0:igdtListLine>
      <ns0:igdtListLine>
        <ns0:igdtQuantity>2 EL</ns0:igdtQuantity>
