  <ns0:recipeName>Gr&#252;nkohl-Chips mit Parmesan</ns0:recipeName>
  <ns0:recipeKeywords />
  <ns0:recipeIntro />
  <ns0:recipeIngredients>
    <ns0:igdtList>
      <ns0:igdtListName />
      <ns0:igdtListLine>
        <ns0:igdtQuantity>250 g</ns0:igdtQuantity>
        <ns0:igdtName ref="gr&#252;nkohl">Gr&#252;nkohl, frisch</ns0:igdtName>
      </ns0:igdtListLine>
      <ns0:igdtListLine>
        <ns0:igdtQuantity>50 g</ns0:igdtQuantity>
        <ns0:igdtName ref="parmesan">Parmesan</ns0:igdtName>
      </ns0:igdtListLine>
      <ns0:igdtListLine>
        <ns0:igdtQuantity>etwas</ns0:igdtQuantity>
        <ns0:igdtName ref="oliven&#246;l">Oliven&#246;l</ns0:igdtName>
      </ns0:igdtListLine>
    </ns0:igdtList>
  </ns0:recipeIngredients>
  <ns0:recipeInstructions>
    <ns0:instruction>
      <ns0:instrStepName />
      <ns0:instrStepText>Den Gr&#252;nkohl waschen und auf ein Backblech zupfen. Dann den Parmesan nach Bedarf dr&#252;ber reiben und mit ein paar Tropfen Oliven&#246;l vermengen.
Dann bei ca. 180 - 200 Grad so lange backen, bis der Gr&#252;nkohl getrocknet ist. Das dauert ca. 10 - 15 Minuten.</ns0:instrStepText>
    </ns0:instruction>
  </ns0:recipeInstructions>
