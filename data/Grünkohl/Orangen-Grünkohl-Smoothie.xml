  <ns0:recipeName>Orangen-Gr&#252;nkohl-Smoothie</ns0:recipeName>
  <ns0:recipeKeywords />
  <ns0:recipeIntro />
  <ns0:recipeIngredients>
    <ns0:igdtList>
      <ns0:igdtListName />
      <ns0:igdtListLine>
        <ns0:igdtQuantity>2</ns0:igdtQuantity>
