  <ns0:recipeName>Gr&#252;ner Herbst-Smoothie</ns0:recipeName>
  <ns0:recipeKeywords />
  <ns0:recipeIntro />
  <ns0:recipeIngredients>
    <ns0:igdtList>
      <ns0:igdtListName />
      <ns0:igdtListLine>
        <ns0:igdtQuantity>50 g</ns0:igdtQuantity>
        <ns0:igdtName ref="spinat">Spinat</ns0:igdtName>
      </ns0:igdtListLine>
      <ns0:igdtListLine>
        <ns0:igdtQuantity>50 g</ns0:igdtQuantity>
        <ns0:igdtName ref="gr&#252;nkohl">Gr&#252;nkohl</ns0:igdtName>
      </ns0:igdtListLine>
      <ns0:igdtListLine>
        <ns0:igdtQuantity>1 kleiner</ns0:igdtQuantity>
        <ns0:igdtName ref="apfel">Apfel</ns0:igdtName>
      </ns0:igdtListLine>
      <ns0:igdtListLine>
        <ns0:igdtQuantity>2</ns0:igdtQuantity>
        <ns0:igdtName ref="feige">Feige(n), frisch</ns0:igdtName>
      </ns0:igdtListLine>
      <ns0:igdtListLine>
        <ns0:igdtQuantity>350 ml</ns0:igdtQuantity>
        <ns0:igdtName ref="">Wasser, still</ns0:igdtName>
      </ns0:igdtListLine>
    </ns0:igdtList>
  </ns0:recipeIngredients>
  <ns0:recipeInstructions>
    <ns0:instruction>
      <ns0:instrStepName />
      <ns0:instrStepText>Den Gr&#252;nkohl und den Spinat waschen und in den Mixer geben. Den Apfel und die Feigen entstielen und die Bl&#252;te entfernen. In St&#252;cke schneiden und dazugeben. Wasser einf&#252;llen und am besten im Hochleistungsmixer das Programm green smoothie w&#228;hlen.</ns0:instrStepText>
    </ns0:instruction>
  </ns0:recipeInstructions>
