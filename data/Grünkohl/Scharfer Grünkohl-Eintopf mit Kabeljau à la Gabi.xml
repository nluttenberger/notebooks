<ns0:recipe xmlns:ns0="http://fruschtique.de/ns/recipe" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://fruschtique.de/ns/recipe file:///c:/Users/nlutt/Documents/Websites/tools/recipe.xsd" rcpID="ck-a896e9f3-e4e6-4b39-b14d-8132f91bfdb0">
  <ns0:meta>
    <ns0:book />
    <ns0:chapter />
