  <ns0:recipeName>Finnischer Kartoffelauflauf mit Wei&#223;kohl oder Gr&#252;nkohl</ns0:recipeName>
  <ns0:recipeKeywords />
  <ns0:recipeIntro />
  <ns0:recipeIngredients>
    <ns0:igdtList>
      <ns0:igdtListName />
      <ns0:igdtListLine>
        <ns0:igdtQuantity>750 g</ns0:igdtQuantity>
        <ns0:igdtName ref="kartoffel">Kartoffel(n)</ns0:igdtName>
      </ns0:igdtListLine>
      <ns0:igdtListLine>
        <ns0:igdtQuantity>250 g</ns0:igdtQuantity>
        <ns0:igdtName ref="schmand">Schmand</ns0:igdtName>
      </ns0:igdtListLine>
      <ns0:igdtListLine>
        <ns0:igdtQuantity />
        <ns0:igdtName ref="pfeffer">Salz und Pfeffer</ns0:igdtName>
      </ns0:igdtListLine>
      <ns0:igdtListLine>
        <ns0:igdtQuantity>2 Bund</ns0:igdtQuantity>
        <ns0:igdtName ref="dill">Dill, gehackt</ns0:igdtName>
      </ns0:igdtListLine>
      <ns0:igdtListLine>
        <ns0:igdtQuantity>&#189; TL</ns0:igdtQuantity>
        <ns0:igdtName ref="k&#252;mmel">K&#252;mmel</ns0:igdtName>
      </ns0:igdtListLine>
      <ns0:igdtListLine>
        <ns0:igdtQuantity>750 g</ns0:igdtQuantity>
        <ns0:igdtName ref="gr&#252;nkohl">Wei&#223;kohl, oder Gr&#252;nkohl</ns0:igdtName>
      </ns0:igdtListLine>
