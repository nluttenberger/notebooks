  <ns0:recipeName>Gr&#252;nkohl-Feldsalat-Gurke-M&#246;hren-Orangen-Smoothie mit Ingwer und Minze</ns0:recipeName>
  <ns0:recipeKeywords />
  <ns0:recipeIntro />
  <ns0:recipeIngredients>
    <ns0:igdtList>
      <ns0:igdtListName />
      <ns0:igdtListLine>
