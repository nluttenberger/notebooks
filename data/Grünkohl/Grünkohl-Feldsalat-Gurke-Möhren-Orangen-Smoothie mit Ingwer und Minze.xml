      </ns0:igdtListLine>
      <ns0:igdtListLine>
        <ns0:igdtQuantity>2 Handvoll</ns0:igdtQuantity>
        <ns0:igdtName ref="feldsalat">Feldsalat</ns0:igdtName>
      </ns0:igdtListLine>
      <ns0:igdtListLine>
        <ns0:igdtQuantity>&#189;</ns0:igdtQuantity>
        <ns0:igdtName ref="gurke">Gurke(n)</ns0:igdtName>
      </ns0:igdtListLine>
      <ns0:igdtListLine>
        <ns0:igdtQuantity>1</ns0:igdtQuantity>
