<ns0:recipe xmlns:ns0="http://fruschtique.de/ns/recipe" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://fruschtique.de/ns/recipe file:///c:/Users/nlutt/Documents/Websites/tools/recipe.xsd" rcpID="ck-0b866c08-b9b8-40c8-a12e-7f38d9d97fc1">
  <ns0:meta>
    <ns0:book />
    <ns0:chapter />
